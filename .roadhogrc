{
  "entry": "src/index.js",
  "disableCSSModules": false,
  "less": false,
  "publicPath": "/",
  "autoprefixer": null,
  "proxy": null,
  "extraBabelPlugins": [
    "transform-runtime"
  ],
  "proxy": {
    "/api": {
      "target": "http://localhost:7001/",
      "changeOrigin": true,
      "pathRewrite": { "^/api" : "api" }
    }
  },
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr"
      ]
    }
  }
}
